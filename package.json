{
  "name": "dimensions",
  "version": "0.1.0",
  "description": "Playground for forces & n-dimensional geometry",
  "author": "aburner",
  "license": "MIT",
  "main": "src/index.tsx",
  "engine": {
    "node": "9.1.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "clean": "rm -rf docs .awcache node_modules",
    "build": "rm -rf docs/* && webpack -p",
    "watch": "webpack-dev-server",
    "prettier": "prettier --write 'src/**/*.+(ts|tsx|js|json)'"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "parser": "typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexburner/dimensions.git"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "three": "0.88.0",
    "three-trackballcontrols": "0.0.6"
  },
  "devDependencies": {
    "@types/lodash": "4.14.85",
    "@types/react": "16.0.22",
    "@types/react-dom": "16.0.3",
    "@types/three": "0.84.30",
    "awesome-typescript-loader": "3.3.0",
    "copy-webpack-plugin": "4.2.0",
    "html-webpack-include-assets-plugin": "1.0.2",
    "html-webpack-plugin": "2.30.1",
    "prettier": "1.8.2",
    "source-map-loader": "0.2.3",
    "tslint": "5.8.0",
    "tslint-config-prettier": "1.6.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.2.0",
    "typescript": "2.6.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "worker-loader": "1.1.0"
  }
}
